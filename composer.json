{
    "name": "networkrailbusinesssystems/support-page",
    "description": "Allows you quickly add a support page to your project",
    "keywords": [
        "support",
        "support page",
        "help",
        "help page",
        "blade template"
    ],
    "type": "library",
    "homepage": "https://github.com/Network-Rail-Business-Systems/support-page",
    "license": "MIT",
    "authors": [
        {
            "name": "Sim Roberts",
            "email": "simran.roberts@networkrail.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "anthonyedmonds/govuk-laravel": "^4",
        "illuminate/support": "^11",
        "laracasts/flash": "^3",
        "spatie/laravel-permission": "^6"
    },
    "require-dev": {
        "anthonyedmonds/laravel-testing-traits": "^0.5.0",
        "larastan/larastan": "^2",
        "laravel/pint": "^1",
        "networkrailbusinesssystems/badges": "^1.1",
        "orchestra/testbench": "^9",
        "phpunit/phpunit": "^11"
    },
    "autoload": {
        "psr-4": {
            "NetworkRailBusinessSystems\\SupportPage\\": "src/app",
            "NetworkRailBusinessSystems\\SupportPage\\Database\\Factories\\": "src/database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NetworkRailBusinessSystems\\SupportPage\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NetworkRailBusinessSystems\\SupportPage\\Providers\\SupportPageProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
